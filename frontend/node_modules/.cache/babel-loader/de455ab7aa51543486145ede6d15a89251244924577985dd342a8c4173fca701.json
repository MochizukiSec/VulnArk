var percentage = total > 0 ? Math.round(value / total * 100) : 0;\n              return \"\".concat(label, \": \").concat(value, \" (\").concat(percentage, \"%)\");\n            }\n          }\n        }\n      }\n    };\n    var doughnutChartOptions = {\n      responsive: true,\n      maintainAspectRatio: false,\n      cutout: '65%',\n      plugins: {\n        legend: {\n          position: 'right',\n          labels: {\n            usePointStyle: true,\n            padding: 20,\n            font: {\n              size: 12\n            }\n          }\n        },\n        tooltip: {\n          callbacks: {\n            label: function label(context) {\n              var label = context.label || '';\n              var value = context.raw || 0;\n              var total = context.dataset.data.reduce(function (a, b) {\n                return a + b;\n              }, 0);\n              var percentage = total > 0 ? Math.round(value / total * 100) : 0;\n              return \"\".concat(label, \": \").concat(value, \" (\").concat(percentage, \"%)\");\n            }\n          }\n        }\n      }\n    };\n    var lineChartOptions = {\n      responsive: true,\n      maintainAspectRatio: false,\n      plugins: {\n        legend: {\n          position: 'top',\n          labels: {\n            usePointStyle: true,\n            padding: 20,\n            font: {\n              size: 12\n            }\n          }\n        }\n      },\n      scales: {\n        y: {\n          beginAtZero: true,\n          ticks: {\n            precision: 0\n          }\n        }\n      }\n    };\n\n    // 获取特定状态的漏洞计数\n    var getStatusCount = function getStatusCount(status) {\n      var statusCounts = dashboardData.value.vulnerabilitiesByStatus || {};\n      return statusCounts[status] || 0;\n    };\n\n    // 获取严重和高危漏洞的总数\n    var getCriticalHighCount = function getCriticalHighCount() {\n      var severityCounts = dashboardData.value.vulnerabilitiesBySeverity || {};\n      return (severityCounts.critical || 0) + (severityCounts.high || 0);\n    };\n\n    // 计算系统占比百分比\n    var getSystemPercentage = function getSystemPercentage(count) {\n      var totalVulnerabilities = dashboardData.value.totalVulnerabilities || 0;\n      return totalVulnerabilities > 0 ? count / totalVulnerabilities * 100 : 0;\n    };\n\n    // 为系统分配颜色\n    var getSystemColor = function getSystemColor(index) {\n      var colors = ['#409EFF', '#67C23A', '#E6A23C', '#F56C6C', '#909399'];\n      return colors[index % colors.length];\n    };\n\n    // 页面跳转\n    var goToVulnerabilities = function goToVulnerabilities() {\n      router.push('/vulnerabilities');\n    };\n    var goToCreateVulnerability = function goToCreateVulnerability() {\n      router.push('/vulnerabilities/create');\n    };\n\n    // 获取仪表盘数据\n    var fetchDashboardData = /*#__PURE__*/function () {\n      var _ref = _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              loading.value = true;\n              _context.prev = 1;\n              _context.next = 4;\n              return store.dispatch('dashboard/fetchDashboardData');\n            case 4:\n              _context.next = 8;\n              break;\n            case 6:\n              _context.prev = 6;\n              _context.t0 = _context[\"catch\"](1);\n            case 8:\n              _context.prev = 8;\n              loading.value = false;\n              return _context.finish(8);\n            case 11:\n            case \"end\":\n              return _context.stop();\n          }\n        }, _callee, null, [[1, 6, 8, 11]]);\n      }));\n      return function fetchDashboardData() {\n        return _ref.apply(this, arguments);\n      };\n    }();\n\n    // 初始化\n    onMounted(function () {\n      fetchDashboardData();\n    });\n    return {\n      loading: loading,\n      dashboa