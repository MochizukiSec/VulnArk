container green\">\n              <i class=\"el-icon-check\"></i>\n            </div>\n            <div class=\"stat-info\">\n              <div class=\"stat-value\">{{ getStatusCount('resolved') }}</div>\n              <div class=\"stat-label\">已解决漏洞</div>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <el-col :xs=\"24\" :sm=\"12\" :md=\"6\">\n        <el-card shadow=\"hover\" class=\"stat-card\">\n          <div class=\"stat-card-content\">\n            <div class=\"stat-icon-container blue\">\n              <i class=\"el-icon-document\"></i>\n            </div>\n            <div class=\"stat-info\">\n              <div class=\"stat-value\">{{ getCriticalHighCount() }}</div>\n              <div class=\"stat-label\">高危漏洞</div>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n    \n    <el-row :gutter=\"20\" class=\"chart-row\">\n      <!-- 漏洞严重程度统计图 -->\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\">\n        <el-card shadow=\"hover\" class=\"chart-card\">\n          <div class=\"chart-header\">\n            <h3 class=\"chart-title\">漏洞严重程度分布</h3>\n          </div>\n          <div class=\"chart-container\" v-loading=\"loading\">\n            <pie-chart \n              v-if=\"!loading && hasSeverityData\" \n              :chart-data=\"severityChartData\" \n              :options=\"pieChartOptions\" \n            />\n            <div v-else-if=\"!loading && !hasSeverityData\" class=\"no-data\">\n              <i class=\"el-icon-data-analysis\"></i>\n              <p>暂无数据</p>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <!-- 漏洞状态统计图 -->\n      <el-col :xs=\"24\" :sm=\"24\" :md=\"12\">\n        <el-card shadow=\"hover\" class=\"chart-card\">\n          <div class=\"chart-header\">\n            <h3 class=\"chart-title\">漏洞状态分布</h3>\n          </div>\n          <div class=\"chart-container\" v-loading=\"loading\">\n            <doughnut-chart \n              v-if=\"!loading && hasStatusData\" \n              :chart-data=\"statusChartData\"\n              :options=\"doughnutChartOptions\"\n            />\n            <div v-else-if=\"!loading && !hasStatusData\" class=\"no-data\">\n              <i class=\"el-icon-data-analysis\"></i>\n              <p>暂无数据</p>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n    \n    <el-row :gutter=\"20\" class=\"chart-row\">\n      <!-- 月度漏洞趋势图 -->\n      <el-col :xs=\"24\">\n        <el-card shadow=\"hover\" class=\"chart-card\">\n          <div class=\"chart-header\">\n            <h3 class=\"chart-title\">月度漏洞趋势</h3>\n          </div>\n          <div class=\"chart-container\" v-loading=\"loading\">\n            <line-chart \n              v-if=\"!loading && hasMonthlyData\" \n              :chart-data=\"monthlyChartData\"\n              :options=\"lineChartOptions\"\n            />\n            <div v-else-if=\"!loading && !hasMonthlyData\" class=\"no-data\">\n              <i class=\"el-icon-data-analysis\"></i>\n              <p>暂无数据</p>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n    \n    <el-row :gutter=\"20\" class=\"chart-row\">\n      <!-- 最近漏洞列表 -->\n      <el-col :xs=\"24\" :md=\"16\">\n        <el-card shadow=\"hover\" class=\"recent-vulns-card\">\n          <div class=\"card-header\">\n            <h3 class=\"card-title\">最近漏洞</h3>\n            <el-button \n              type=\"text\" \n              @click=\"goToVulnerabilities\" \n              class=\"view-all-btn\"\n            >\n              查看全部\n            </el-button>\n          </div>\n          <div v-loading=\"loading\">\n            <el-table \n              v-if=\"!loading && hasRecentVulnerabilities\"\n              :data=\"dashboardData.recentVulnerabilities\" \n              style=\"width: 100%\"\n              size=\"medium\"\n              :header-cell-style=\"{background:'#f5f7fa'}\"\n            >\n              <el-table-column label=\"标题\" prop=\"title\" min-width=\"200\">\n                <template #default=\"scope\">\n                  <router-link :to=\"`/vulnerabilities/${scope.row.id}`\" class=\"vuln-title-link\">\n                    {{ scope.row.title }}\n                  </router-link>\n                </template>\n              </el-table-column>\n              <el-table-column label=\"严重程度\" prop=\"severity\" width=\"100\">\n                <template #default=\"scope\">\n                  <el-tag :type=\"$filters.severityClass(scope.row.severity)\" size=\"small\">\n                    {{ $filters.severityText(scope.row.severity) }}\n                  </el-tag>\n                </template>\n              </el-table-column>\n              <el-table-column label=\"状态\" prop=\"status\" width=\"100\">\n                <template #default=\"scope\">\n                  <el-tag :type=\"$filters.statusClass(scope.row.status)\" size=\"small\">\n                    {{ $filters.statusText(scope.row.status) }}\n                  </el-tag>\n                </template>\n              </el-table-column>\n              <el-table-column label=\"创建时间\" prop=\"createdAt\" width=\"150\">\n                <template #default=\"scope\">\n                  {{ $filters.formatDate(scope.row.createdAt) }}\n                </template>\n              </el-table-column>\n            </el-table>\n            <div v-else-if=\"!loading && !hasRecentVulnerabilities\" class=\"no-data\">\n              <i class=\"el-icon-document\"></i>\n              <p>暂无漏洞数据</p>\n              <el-button type=\"primary\" size=\"small\" @click=\"goToCreateVulnerability\">\n                创建漏洞\n              </el-button>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n      \n      <!-- 受影响最多的系统 -->\n      <el-col :xs=\"24\" :md=\"8\">\n        <el-card shadow=\"hover\" class=\"systems-card\">\n          <div class=\"card-header\">\n            <h3 class=\"card-title\">受影响最多的系统</h3>\n          </div>\n          <div v-loading=\"loading\">\n            <div v-if=\"!loading && hasTopAffectedSystems\" class=\"systems-list\">\n              <div \n                v-for=\"(system, index) in dashboardData.topAffectedSystems\" \n                :key=\"index\"\n                class=\"system-item\"\n              >\n                <div class=\"system-info\">\n                  <div class=\"system-name\">{{ system.system }}</div>\n                  <div class=\"system-count\">{{ system.count }} 个漏洞</div>\n                </div>\n                <el-progress \n                  :percentage=\"getSystemPercentage(system.count)\" \n                  :color=\"getSystemColor(index)\"\n                  :show-text=\"false\"\n                  :stroke-width=\"8\"\n                />\n              </div>\n            </div>\n            <div v-else-if=\"!loading && !hasTopAffectedSystems\" class=\"no-data\">\n              <i class=\"el-icon-cpu\"></i>\n              <p>暂无系统数据</p>\n            </div>\n          </div>\n        </el-card>\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport { computed, onMounted, ref } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRouter } from 'vue-router'\nimport { \n  Chart as ChartJS, \n  Title, \n  Tooltip, \n  Legend, \n  ArcElement, \n  CategoryScale, \n  LinearScale,\n  PointElement, \n  LineElement,\n  BarElement\n} from 'chart.js'\nimport { Pie as PieChart, Doughnut as DoughnutChart, Line as LineChart } from 'vue-chartjs'\n\n// 注册Chart.js组件\nChartJS.register(\n  Title, \n  Tooltip, \n  Legend, \n  ArcElement, \n  CategoryScale, \n  LinearScale,\n  PointElement, \n  LineElement,\n  BarElement\n)\n\nexport default {\n  name: 'Dashboard',\n  components: {\n    PieChart,\n    DoughnutChart,\n    LineChart\n  },\n  \n  setup() {\n    const store = useStore()\n    const router = useRouter()\n    const loading = ref(true)\n    \n    // 仪表盘数据\n    const dashboardData = computed(() => store.state.dashboard?.data || {})\n    \n    // 检查是否有各种数据\n    const hasSeverityData = computed(() => {\n      const severityCounts = dashboardData.value.vulnerabilitiesBySeverity || {}\n      return Object.values(severityCounts).some(count => count > 0)\n    })\n    \n    const hasStatusData = computed(() => {\n      const statusCounts = dashboardData.value.vulnerabilitiesByStatus || {}\n      return Object.values(statusCounts).some(count => count > 0)\n    })\n    \n    const hasMonthlyData = computed(() => {\n      return (dashboardData.value.vulnerabilitiesByMonth || []).length > 0\n    })\n    \n    const hasRecentVulnerabilities = computed(() => {\n      return (dashboardData.value.recentVulnerabilities || []).length > 0\n    })\n    \n    const hasTopAffectedSystems = computed(() => {\n      return (dashboardData.value.topAffectedSystems || []).length > 0\n    })\n    \n    // 图表数据\n    const severityChartData = computed(() => {\n      const severityCounts = dashboardData.value.vulnerabilitiesBySeverity || {}\n      return {\n        labels: [\n          '严重', \n          '高危', \n          '中危', \n          '低危',\n          '信息'\n        ],\n        datasets: [\n          {\n            backgroundColor: ['#F56C6C', '#E6A23C', '#F2D864', '#67C23A', '#909399'],\n            data: [\n              severityCounts.critical || 0,\n              severityCounts.high || 0,\n              severityCounts.medium || 0,\n              severityCounts.low || 0,\n              severityCounts.info || 0\n            ]\n          }\n        ]\n      }\n    })\n    \n    const statusChartData = computed(() => {\n      const statusCounts = dashboardData.value.vulnerabilitiesByStatus || {}\n      return {\n        labels: [\n          '开放', \n          '处理中', \n          '已解决', \n          '已关闭',\n          '误报'\n        ],\n        datasets: [\n          {\n            backgroundColor: ['#F56C6C', '#E6A23C', '#67C23A', '#909399', '#909399'],\n            data: [\n              statusCounts.open || 0,\n              statusCounts.in_progress || 0,\n              statusCounts.resolved || 0,\n              statusCounts.closed || 0,\n              statusCounts.false_positive || 0\n            ]\n          }\n        ]\n      }\n    })\n    \n    const monthlyChartData = computed(() => {\n      const monthlyData = dashboardData.value.vulnerabilitiesByMonth || []\n      \n      // 处理月度数据\n      return {\n        labels: monthlyData.map(item => `${item.month} ${item.year}`),\n        datasets: [\n          {\n            label: '严重',\n            backgroundColor: 'rgba(245, 108, 108, 0.2)',\n            borderColor: '#F56C6C',\n            borderWidth: 2,\n            tension: 0.4,\n            data: monthlyData.map(item => item.severities.critical || 0)\n          },\n          {\n            label: '高危',\n            backgroundColor: 'rgba(230, 162, 60, 0.2)',\n            borderColor: '#E6A23C',\n            borderWidth: 2,\n            tension: 0.4,\n            data: monthlyData.map(item => item.severities.high || 0)\n          },\n          {\n            label: '中危',\n            backgroundColor: 'rgba(242, 216, 100, 0.2)',\n            borderColor: '#F2D864',\n            borderWidth: 2,\n            tension: 0.4,\n            data: monthlyData.map(item => item.severities.medium || 0)\n          },\n          {\n            label: '低危和信息',\n            backgroundColor: 'rgba(103, 194, 58, 0.2)',\n            borderColor: '#67C23A',\n            borderWidth: 2,\n            tension: 0.4,\n            data: monthlyData.map(item => (item.severities.low || 0) + (item.severities.info || 0))\n          }\n        ]\n      }\n    })\n    \n    // 图表配置\n    const pieChartOptions = {\n      responsive: true,\n      maintainAspectRatio: false,\n      plugins: {\n        legend: {\n          position: 'right',\n          labels: {\n            usePointStyle: true,\n            padding: 20,\n            font: {\n              size: 12\n      