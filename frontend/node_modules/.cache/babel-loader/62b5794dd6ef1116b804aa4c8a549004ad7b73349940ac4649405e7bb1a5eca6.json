: relative;\n  \n  &:hover {\n    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.08);\n  }\n  \n  .search-form {\n    display: flex;\n    flex-wrap: wrap;\n    gap: 16px;\n    \n    .el-form-item {\n      margin-bottom: 0;\n      margin-right: 0;\n    }\n  }\n  \n  .search-btn, .reset-btn {\n    border-radius: 8px;\n    transition: all 0.3s;\n    \n    i {\n      margin-right: 4px;\n    }\n  }\n  \n  .search-btn:hover {\n    transform: translateY(-2px);\n    box-shadow: 0 4px 12px rgba(64, 158, 255, 0.2);\n  }\n}\n\n.vuln-table-card {\n  background: white;\n  border-radius: 12px;\n  box-shadow: 0 4px 15px rgba(0, 0, 0, 0.03);\n  padding: 20px;\n  transition: all 0.3s ease;\n  position: relative;\n  \n  &:hover {\n    box-shadow: 0 8px 25px rgba(0, 0, 0, 0.08);\n  }\n  \n  &::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 4px;\n    background: linear-gradient(90deg, #409EFF, #53A8FF);\n    border-radius: 12px 12px 0 0;\n  }\n  \n  .el-table {\n    border-radius: 8px;\n    overflow: hidden;\n    margin-bottom: 16px;\n    \n    // 美化表格头部\n    ::v-deep(th) {\n      background-color: #f5f7fa;\n      color: #606266;\n      font-weight: 600;\n      padding: 12px 0;\n    }\n    \n    // 美化表格单元格\n    ::v-deep(td) {\n      padding: 12px 0;\n    }\n    \n    // 美化表格行悬停状态\n    ::v-deep(.el-table__row) {\n      transition: all 0.2s;\n      \n      &:hover {\n        background-color: #f0f9ff;\n      }\n    }\n  }\n}\n\n.vuln-link {\n  color: #409EFF;\n  text-decoration: none;\n  transition: color 0.3s ease;\n  font-weight: 500;\n  display: block;\n  \n  &:hover {\n    color: #66b1ff;\n    text-decoration: underline;\n  }\n}\n\n.cve-id {\n  font-family: monospace;\n  color: #606266;\n  font-weight: 500;\n  background-color: rgba(0, 0, 0, 0.03);\n  padding: 2px 6px;\n  border-radius: 4px;\n}\n\n.cvss-score {\n  font-weight: 600;\n  padding: 2px 8px;\n  border-radius: 4px;\n  display: inline-block;\n  \n  &.critical {\n    colo