n-card\">\n          <div class=\"login-header\">\n            <div class=\"logo-container\">\n              <!-- 替换Logo图片为图标 -->\n              <div class=\"logo-icon\">\n                <i class=\"el-icon-s-help\"></i>\n              </div>\n            </div>\n            <h1 class=\"login-title\">漏洞管理平台</h1>\n            <p class=\"login-subtitle\">登录您的账号以管理安全漏洞</p>\n          </div>\n          \n          <el-form \n            ref=\"loginForm\" \n            :model=\"loginData\" \n            :rules=\"rules\" \n            label-position=\"top\" \n            @submit.prevent=\"handleLogin\"\n          >\n            <el-alert \n              v-if=\"error\" \n              :title=\"error\" \n              type=\"error\" \n              show-icon \n              :closable=\"false\"\n              style=\"margin-bottom: 24px\"\n            />\n              \n            <el-form-item label=\"邮箱\" prop=\"email\">\n              <el-input \n                v-model=\"loginData.email\" \n                placeholder=\"请输入邮箱\" \n                type=\"email\"\n                prefix-icon=\"el-icon-message\"\n                @keyup.enter=\"submitForm\"\n                class=\"custom-input\"\n              />\n            </el-form-item>\n            \n            <el-form-item label=\"密码\" prop=\"password\">\n              <el-input \n                v-model=\"loginData.password\" \n                placeholder=\"请输入密码\" \n                type=\"password\"\n                prefix-icon=\"el-icon-lock\"\n                show-password\n                @keyup.enter=\"submitForm\"\n                class=\"custom-input\"\n              />\n            </el-form-item>\n            \n            <div class=\"form-options\">\n              <el-checkbox v-model=\"rememberMe\">记住我</el-checkbox>\n              <el-link type=\"primary\" :underline=\"false\">忘记密码?</el-link>\n            </div>\n            \n            <div class=\"form-actions\">\n              <el-button \n                type=\"primary\" \n                :loading=\"loading\" \n                @click=\"submitForm\" \n                class=\"submit-button\"\n              >\n                登录\n              </el-button>\n            </div>\n            \n            <div class=\"form-footer\">\n              <div class=\"divider\">\n                <span>或者</span>\n              </div>\n              <div class=\"social-login\">\n                <button class=\"social-btn github\">\n                  <i class=\"el-icon-s-platform\"></i>\n                </button>\n                <button class=\"social-btn google\">\n                  <i class=\"el-icon-s-promotion\"></i>\n                </button>\n                <button class=\"social-btn microsoft\">\n                  <i class=\"el-icon-s-grid\"></i>\n                </button>\n              </div>\n              <p class=\"register-text\">还没有账号？<router-link to=\"/register\" class=\"register-link\">立即注册</router-link></p>\n            </div>\n          </el-form>\n        </div>\n\n        <div class=\"login-hero\">\n          <h2 class=\"hero-title\">智能安全管理系统</h2>\n          <p class=\"hero-subtitle\">全面、直观的漏洞管理平台，让安全管理更简单高效</p>\n          <div class=\"hero-actions\">\n            <button class=\"hero-btn primary-btn\">\n              <span>立即开始</span>\n              <i class=\"el-icon-arrow-right\"></i>\n            </button>\n            <button class=\"hero-btn secondary-btn\">\n              <span>了解更多</span>\n              <i class=\"el-icon-d-arrow-right\"></i>\n            </button>\n          </div>\n        </div>\n      </div>\n      \n      <div class=\"login-features\">\n        <div class=\"feature-card\">\n          <div class=\"feature-icon\">\n            <i class=\"el-icon-warning-outline\"></i>\n          </div>\n          <h3>漏洞跟踪</h3>\n          <p>全面管理系统安全漏洞，支持CVSS评分评估</p>\n          <ul class=\"feature-list\">\n            <li><span class=\"feature-highlight\">智能分类</span> 风险自动评估</li>\n          </ul>\n        </div>\n        <div class=\"feature-card\">\n          <div class=\"feature-icon\">\n            <i class=\"el-icon-data-analysis\"></i>\n          </div>\n          <h3>数据分析</h3>\n          <p>安全数据可视化，帮助快速识别安全趋势</p>\n          <ul class=\"feature-list\">\n            <li><span class=\"feature-highlight\">实时仪表盘</span> 核心指标监控</li>\n          </ul>\n        </div>\n        <div class=\"feature-card\">\n          <div class=\"feature-icon\">\n            <i class=\"el-icon-bell\"></i>\n          </div>\n          <h3>实时提醒</h3>\n          <p>多渠道智能提醒，确保安全事件响应及时</p>\n          <ul class=\"feature-list\">\n            <li><span class=\"feature-highlight\">团队协作</span> 快速跟踪处理</li>\n          </ul>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive, computed, onMounted } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRouter, useRoute } from 'vue-router'\n\nexport default {\n  name: 'Login',\n  \n  setup() {\n    const store = useStore()\n    const router = useRouter()\n    const route = useRoute()\n    const loginForm = ref(null)\n    const rememberMe = ref(false)\n    \n    // 表单数据\n    const loginData = reactive({\n      email: '',\n      password: ''\n    })\n    \n    // 表单验证规则\n    const rules = {\n      email: [\n        { required: true, message: '请输入邮箱', trigger: 'blur' },\n        { type: 'email', message: '请输入正确的邮箱格式', trigger: 'blur' }\n      ],\n      password: [\n        { required: true, message: '请输入密码', trigger: 'blur' },\n        { min: 8, message: '密码长度不能少于8个字符', trigger: 'blur' }\n      ]\n    }\n    \n    // 从store中获取状态\n    const loading = computed(() => store.getters['auth/authLoading'])\n    const error = computed(() => store.getters['auth/authError'])\n    \n    // 提交表单\n    const submitForm = async () => {\n      if (!loginForm.value) return\n      \n      await loginForm.value.validate(async (valid) => {\n        if (valid) {\n          await handleLogin()\n        }\n      })\n    }\n    \n    // 处理登录逻辑\n    const handleLogin = async () => {\n      const result = await store.dispatch('auth/login', loginData)\n      \n      if (result.success) {\n        // 重定向到指定页面或默认到仪表盘\n        const redirectPath = route.query.redirect || '/dashboard'\n        router.push(redirectPath)\n      }\n    }\n    \n    // 初始化时清除错误\n    onMounted(() => {\n      store.dispatch('auth/authError', null)\n    })\n    \n    return {\n      loginForm,\n      loginData,\n      rules,\n      loading,\n      error,\n      rememberMe,\n      submitForm,\n      handleLogin\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.login-container {\n  min-height: 100vh;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background: linear-gradient(125deg, #000428 0%, #004e92 100%);\n  padding: 20px;\n  position: relative;\n  overflow: hidden;\n  \n  &::before, &::after {\n    content: '';\n    position: absolute;\n    border-radius: 50%;\n    filter: blur(40px);\n  }\n  \n  &::before {\n    width: 30vw;\n    height: 30vw;\n    background: rgba(255, 0, 128, 0.2);\n    top: -5%;\n    right: -5%;\n  }\n  \n  &::after {\n    width: 35vw;\n    height: 35vw;\n    background: rgba(0, 255, 255, 0.2);\n    bottom: -10%;\n    left: -10%;\n  }\n}\n\n.background-decoration {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  pointer-events: none;\n  z-index: 0;\n  overflow: hidden;\n}\n\n.login-content {\n  display: flex;\n  width: 100%;\n  max-width: 1200px;\n  z-index: 1;\n  gap: 40px;\n  align-items: stretch;\n  position: relative;\n  flex-direction: column;\n  \n  @media (max-width: 1024px) {\n    flex-direction: column;\n  }\n}\n\n.login-main {\n  display: flex;\n  flex-direction: row;\n  width: 100%;\n  gap: 40px;\n  \n  @media (max-width: 1024px) {\n    flex-direction: column;\n  }\n}\n\n.login-card {\n  width: 450px;\n  max-width: 450px;\n  flex: 0 0 auto;\n  padding: 40px;\n  background: rgba(20, 20, 40, 0.7);\n  backdrop-filter: blur(20px);\n  border-radius: 24px;\n  box-shadow: \n    0 20px 40px rgba(0, 0, 0, 0.4),\n    0 0 0 1px rgba(255, 255, 255, 0.1) inset,\n    0 0 30px rgba(0, 255, 255, 0.2);\n  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n  animation: fadeIn 0.6s ease-out;\n  position: relative;\n  overflow: hidden;\n  \n  &:hover {\n    transform: translateY(-5px);\n    box-shadow: \n      0 25px 50px rgba(0, 0, 0, 0.5),\n      0 0 0 1px rgba(255, 255, 255, 0.15) inset,\n      0 0 30px rgba(0, 255, 255, 0.3);\n  }\n  \n  /* 霓虹边框效果 */\n  &::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    height: 3px;\n    background: linear-gradient(90deg, #ff00cc, #00ffff, #ff00cc);\n    background-size: 200% 100%;\n    animation: gradient 5s ease infinite;\n  }\n  \n  /* 登录卡片背景装饰 */\n  &::after {\n    content: '';\n    position: absolute;\n    top: 0;\n    right: 0;\n    width: 150px;\n    height: 150px;\n    background: rgba(255, 255, 255, 0.03);\n    background-size: cover;\n    opacity: 0.05;\n    pointer-events: none;\n  }\n  \n  @keyframes gradient {\n    0% {\n      background-position: 0% 50%;\n    }\n    50% {\n      background-position: 100% 50%;\n    }\n    100% {\n      background-position: 0% 50%;\n    }\n  }\n  \n  @media (max-width: 1024px) {\n    width: 100%;\n    max-width: none;\n  }\n}\n\n.login-hero {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n  text-align: center;\n  padding: 40px;\n  background: rgba(20, 20, 40, 0.7);\n  backdrop-filter: blur(20px);\n  border-radius: 24px;\n  box-shadow: \n    0 20px 40px rgba(0, 0, 0, 0.4),\n    0 0 0 1px rgba(255, 255, 255, 0.1) inset,\n    0 0 30px rgba(0, 255, 255, 0.2);\n  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n  animation: fadeIn 0.8s ease-out;\n  position: relative;\n  overflow: hidden;\n  \n  &:hover {\n    transform: translateY(-5px);\n    box-shadow: \n      0 25px 