ter: blur(20px);\n  border-radius: 24px;\n  box-shadow: \n    0 20px 40px rgba(0, 0, 0, 0.4),\n    0 0 0 1px rgba(255, 255, 255, 0.1) inset,\n    0 0 30px rgba(0, 255, 255, 0.2);\n  transition: all 0.4s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n  animation: fadeIn 0.6s ease-out;\n  position: relative;\n  overflow: hidden;\n  \n  &:hover {\n    transform: translateY(-5px) scale(1.01);\n    box-shadow: \n      0 25px 50px rgba(0, 0, 0, 0.5),\n      0 0 0 1px rgba(255, 255, 255, 0.15) inset,\n      0 0 30px rgba(0, 255, 255, 0.3);\n  }\n  \n  /* 霓虹边框效果 */\n  &::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    height: 3px;\n    background: linear-gradient(90deg, #ff00cc, #00ffff, #ff00cc);\n    background-size: 200% 100%;\n    animation: gradient 5s ease infinite;\n  }\n\n  @keyframes gradient {\n    0% {\n      background-position: 0% 50%;\n    }\n    50% {\n      background-position: 100% 50%;\n    }\n    100% {\n      background-position: 0% 50%;\n    }\n  }\n  \n  /* 登录卡片背景装饰 */\n  &::after {\n    content: '';\n    position: absolute;\n    top: 0;\n    right: 0;\n    width: 150px;\n    height: 150px;\n    background-image: url('@/assets/images/card-pattern.svg');\n    background-size: cover;\n    opacity: 0.05;\n    pointer-events: none;\n    filter: hue-rotate(180deg);\n  }\n  \n  @media (max-width: 1024px) {\n    margin: 0 auto;\n    width: 90%;\n  }\n}\n\n.login-features {\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n  animation: slideIn 0.8s cubic-bezier(0.23, 1, 0.32, 1);\n  \n  @media (max-width: 1024px) {\n    display: none;\n  }\n}\n\n.feature-card {\n  background: rgba(20, 20, 40, 0.5);\n  backdrop-filter: blur(10px);\n  padding: 30px;\n  border-radius: 24px;\n  color: white;\n  transition: all 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275);\n  position: relative;\n  overflow: hidden;\n  border: 1px solid rgba(255, 255, 255, 0.1);\n  box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);\n  \n  &:hover {\n    transform: translateY(-5px) scale(1.02);\n    background: rgba(30, 30, 60, 0.5);\n    border-color: rgba(0, 255, 255, 0.3);\n    box-shadow: \n      0 15px 30px rgba(0, 0, 0, 0.3),\n      0 0 15px rgba(0, 255, 255, 0.3);\n  }\n  \n  /* 霓虹边框效果 */\n  &::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 3px;\n    height: 0;\n    background: linear-gradient(0deg, #ff00cc, #00ffff);\n    transition: height 0.4s ease;\n  }\n  \n  &:hover::before {\n    height: 100%;\n  }\n  \n  h3 {\n    font-size: 22px;\n    margin: 15px 0 10px;\n    background: linear-gradient(90deg, #ffffff, #00ffff);\n    -webkit-background-clip: text;\n    -webkit-text-fill-color: transparent;\n    font-weight: 700;\n  }\n  \n  p {\n    margin: 0;\n    opacity: 0.9;\n    line-height: 1.7;\n    font-size: 15px;\n  }\n}\n\n.feature-icon {\n  font-size: 24px;\n  height: 60px;\n  width: 60px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  background: rgba(0, 255, 255, 0.1);\n  border-radius: 16px;\n  overflow: hidden;\n  position: relative;\n  box-shadow: 0 0 20px rgba(0, 255, 255, 0.2);\n  \n  i {\n    font-size: 28px;\n    color: #00ffff;\n    transition: all 0.3s ease;\n  }\n  \n  &:hover i {\n    transform: scale(1.2);\n    color: #ff00cc;\n  }\n  \n  &::after {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    right: 0;\n    bottom: 0;\n    border-radius: 16px;\n    box-shadow: 0 0 10px rgba(0, 255, 255, 0.3) inset;\n    opacity: 0;\n    transition: opacity 0.3s ease;\n  }\n  \n  &:hover::after {\n    opacity: 1;\n  }\n}\n\n.feature-img {\n  width: 100%;\n  height: 100%;\n  object-fit: cover;\n  filter: drop-shadow(0 0 5px rgba(0, 255, 255, 0.5));\n  transition: transform 0.3s ease, filter 0.3s ease;\n  \n  &:hover {\n    transform: scale(1.1);\n    filter: drop-shadow(0 0 8px rgba(0, 255, 255, 0.8));\n  }\n}\n\n.logo-container {\n  display: flex;\n  justify-content: center;\n  margin-bottom: 