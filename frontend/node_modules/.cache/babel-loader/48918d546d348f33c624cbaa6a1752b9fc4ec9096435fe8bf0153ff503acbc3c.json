{"ast":null,"code":"import \"core-js/modules/es.error.to-string.js\";\nimport \"core-js/modules/es.array.concat.js\";\nimport \"core-js/modules/es.array.find-index.js\";\nimport \"core-js/modules/es.array.splice.js\";\nimport \"core-js/modules/es.array.unshift.js\";\nimport \"core-js/modules/es.date.now.js\";\nimport \"core-js/modules/es.date.to-iso-string.js\";\nimport \"core-js/modules/es.date.to-string.js\";\nimport \"core-js/modules/es.function.name.js\";\nimport \"core-js/modules/es.object.to-string.js\";\nimport \"core-js/modules/es.regexp.to-string.js\";\nimport \"core-js/modules/web.timers.js\";\nimport { ref, reactive } from 'vue';\nimport { ElMessage, ElMessageBox } from 'element-plus';\nexport default {\n  name: 'Reports',\n  setup: function setup() {\n    // 加载状态\n    var loading = ref(false);\n    var generating = ref(false);\n\n    // 报告生成表单\n    var reportForm = reactive({\n      type: 'summary',\n      format: 'pdf',\n      dateRange: [],\n      severities: ['critical', 'high', 'medium']\n    });\n\n    // 模拟报告数据\n    var reports = ref([{\n      id: '1',\n      name: '2023年第二季度漏洞摘要报告',\n      type: 'summary',\n      format: 'pdf',\n      createdAt: '2023-07-01T08:30:00Z',\n      url: '#'\n    }, {\n      id: '2',\n      name: '网络安全合规性检查报告',\n      type: 'compliance',\n      format: 'word',\n      createdAt: '2023-06-15T14:20:00Z',\n      url: '#'\n    }, {\n      id: '3',\n      name: '漏洞详细分析报告',\n      type: 'detailed',\n      format: 'excel',\n      createdAt: '2023-06-10T11:45:00Z',\n      url: '#'\n    }]);\n\n    // 获取报告类型文本\n    var getReportTypeText = function getReportTypeText(type) {\n      var typeMap = {\n        summary: '摘要报告',\n        detailed: '详细报告',\n        compliance: '合规报告',\n        trend: '趋势分析'\n      };\n      return typeMap[type] || type;\n    };\n\n    // 获取报告标签类型\n    var getReportTagType = function getReportTagType(type) {\n      var typeMap = {\n        summary: '',\n        detailed: 'success',\n        compliance: 'warning',\n        trend: 'info'\n      };\n      return typeMap[type] || '';\n    };\n\n    // 格式化日期\n    var formatDate = function formatDate(dateString) {\n      var date = new Date(dateString);\n      return date.toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      });\n    };\n\n    // 滚动到报告生成器\n    var scrollToGenerator = function scrollToGenerator() {\n      document.querySelector('.report-card').scrollIntoView({\n        behavior: 'smooth',\n        block: 'start'\n      });\n    };\n\n    // 生成报告\n    var generateReport = function generateReport() {\n      if (!reportForm.dateRange || reportForm.dateRange.length !== 2) {\n        ElMessage.warning('请选择报告时间范围');\n        return;\n      }\n      if (reportForm.severities.length === 0) {\n        ElMessage.warning('请至少选择一个严重程度级别');\n        return;\n      }\n      generating.value = true;\n\n      // 模拟报告生成过程\n      setTimeout(function () {\n        generating.value = false;\n        var reportTypes = {\n          summary: '漏洞摘要报告',\n          detailed: '详细漏洞报告',\n          compliance: '合规性报告',\n          trend: '趋势分析报告'\n        };\n\n        // 创建新报告对象\n        var newReport = {\n          id: Date.now().toString(),\n          name: \"\".concat(new Date().getFullYear(), \"\\u5E74\").concat(reportTypes[reportForm.type]),\n          type: reportForm.type,\n          format: reportForm.format,\n          createdAt: new Date().toISOString(),\n          url: '#'\n        };\n\n        // 添加到报告列表\n        reports.value.unshift(newReport);\n        ElMessage.success('报告生成成功');\n      }, 2000);\n    };\n\n    // 下载报告\n    var downloadReport = function downloadReport(report) {\n      // 实际应用中，这里应该进行实际的下载操作\n      ElMessage.success(\"\\u5F00\\u59CB\\u4E0B\\u8F7D\\uFF1A\".concat(report.name, \".\").concat(report.format));\n    };\n\n    // 确认删除\n    var confirmDelete = function confirmDelete(report) {\n      ElMessageBox.confirm(\"\\u786E\\u5B9A\\u8981\\u5220\\u9664\\u62A5\\u544A\\\"\".concat(report.name, \"\\\"\\u5417\\uFF1F\"), '警告', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(function () {\n        deleteReport(report.id);\n      })[\"catch\"](function () {\n        // 用户取消删除\n      });\n    };\n\n    // 删除报告\n    var deleteReport = function deleteReport(id) {\n      var index = reports.value.findIndex(function (r) {\n        return r.id === id;\n      });\n      if (index !== -1) {\n        reports.value.splice(index, 1);\n        ElMessage.success('报告已删除');\n      }\n    };\n    return {\n      loading: loading,\n      generating: generating,\n      reportForm: reportForm,\n      reports: reports,\n      getReportTypeText: getReportTypeText,\n      getReportTagType: getReportTagType,\n      formatDate: formatDate,\n      generateReport: generateReport,\n      downloadReport: downloadReport,\n      confirmDelete: confirmDelete,\n      scrollToGenerator: scrollToGenerator\n    };\n  }\n};","map":{"version":3,"names":["ref","reactive","ElMessage","ElMessageBox","name","setup","loading","generating","reportForm","type","format","dateRange","severities","reports","id","createdAt","url","getReportTypeText","typeMap","summary","detailed","compliance","trend","getReportTagType","formatDate","dateString","date","Date","toLocaleDateString","year","month","day","hour","minute","scrollToGenerator","document","querySelector","scrollIntoView","behavior","block","generateReport","length","warning","value","setTimeout","reportTypes","newReport","now","toString","concat","getFullYear","toISOString","unshift","success","downloadReport","report","confirmDelete","confirm","confirmButtonText","cancelButtonText","then","deleteReport","index","findIndex","r","splice"],"sources":["/Users/yue/Desktop/vuln-Management/frontend/src/views/reports/Reports.vue"],"sourcesContent":["<template>\n  <div class=\"reports-page\">\n    <!-- 页面标题 -->\n    <div class=\"page-header\">\n      <div class=\"header-content\">\n        <h1 class=\"page-title\">\n          <span class=\"title-icon\"><i class=\"el-icon-document\"></i></span>\n          安全报告\n          <span class=\"title-highlight\">管理</span>\n        </h1>\n        <p class=\"page-subtitle\">查看、生成和导出系统安全漏洞相关报告</p>\n      </div>\n    </div>\n\n    <!-- 报告生成卡片 -->\n    <div class=\"report-card-container\">\n      <el-card shadow=\"hover\" class=\"report-card\">\n        <template #header>\n          <div class=\"card-header\">\n            <h2 class=\"section-title\">\n              <i class=\"el-icon-document-add\"></i>\n              报告生成\n            </h2>\n          </div>\n        </template>\n\n        <div class=\"report-options\">\n          <el-form :model=\"reportForm\" label-position=\"top\">\n            <el-row :gutter=\"24\">\n              <el-col :xs=\"24\" :sm=\"12\">\n                <el-form-item label=\"报告类型\">\n                  <el-select v-model=\"reportForm.type\" placeholder=\"选择报告类型\" style=\"width: 100%\">\n                    <el-option-group label=\"常规报告\">\n                      <el-option label=\"漏洞摘要报告\" value=\"summary\">\n                        <template #default>\n                          <div class=\"report-option\">\n                            <i class=\"el-icon-document option-icon summary\"></i>\n                            <span>漏洞摘要报告</span>\n                          </div>\n                        </template>\n                      </el-option>\n                      <el-option label=\"详细漏洞报告\" value=\"detailed\">\n                        <template #default>\n                          <div class=\"report-option\">\n                            <i class=\"el-icon-document-checked option-icon detailed\"></i>\n                            <span>详细漏洞报告</span>\n                          </div>\n                        </template>\n                      </el-option>\n                    </el-option-group>\n                    <el-option-group label=\"专项报告\">\n                      <el-option label=\"合规性报告\" value=\"compliance\">\n                        <template #default>\n                          <div class=\"report-option\">\n                            <i class=\"el-icon-collection-check option-icon compliance\"></i>\n                            <span>合规性报告</span>\n                          </div>\n                        </template>\n                      </el-option>\n                      <el-option label=\"趋势分析报告\" value=\"trend\">\n                        <template #default>\n                          <div class=\"report-option\">\n                            <i class=\"el-icon-data-line option-icon trend\"></i>\n                            <span>趋势分析报告</span>\n                          </div>\n                        </template>\n                      </el-option>\n                    </el-option-group>\n                  </el-select>\n                </el-form-item>\n              </el-col>\n              <el-col :xs=\"24\" :sm=\"12\">\n                <el-form-item label=\"报告格式\">\n                  <el-select v-model=\"reportForm.format\" placeholder=\"选择输出格式\" style=\"width: 100%\">\n                    <el-option label=\"PDF文档\" value=\"pdf\">\n                      <template #default>\n                        <div class=\"report-option\">\n                          <i class=\"el-icon-document option-icon pdf\"></i>\n                          <span>PDF文档</span>\n                        </div>\n                      </template>\n                    </el-option>\n                    <el-option label=\"Excel工作表\" value=\"excel\">\n                      <template #default>\n                        <div class=\"report-option\">\n                          <i class=\"el-icon-document option-icon excel\"></i>\n                          <span>Excel工作表</span>\n                        </div>\n                      </template>\n                    </el-option>\n                    <el-option label=\"Word文档\" value=\"word\">\n                      <template #default>\n                        <div class=\"report-option\">\n                          <i class=\"el-icon-document option-icon word\"></i>\n                          <span>Word文档</span>\n                        </div>\n                      </template>\n                    </el-option>\n                    <el-option label=\"HTML网页\" value=\"html\">\n                      <template #default>\n                        <div class=\"report-option\">\n                          <i class=\"el-icon-document option-icon html\"></i>\n                          <span>HTML网页</span>\n                        </div>\n                      </template>\n                    </el-option>\n                  </el-select>\n                </el-form-item>\n              </el-col>\n            </el-row>\n\n            <el-form-item label=\"报告时间范围\">\n              <el-date-picker\n                v-model=\"reportForm.dateRange\"\n                type=\"daterange\"\n                range-separator=\"至\"\n                start-placeholder=\"开始日期\"\n                end-placeholder=\"结束日期\"\n                style=\"width: 100%\"\n                class=\"date-range-picker\"\n              />\n            </el-form-item>\n\n            <el-form-item label=\"包含的严重程度\">\n              <div class=\"severity-checkboxes\">\n                <el-checkbox-group v-model=\"reportForm.severities\">\n                  <el-checkbox label=\"critical\">\n                    <span class=\"severity-checkbox\">\n                      <span class=\"severity-dot critical\"></span>\n                      <span>严重</span>\n                    </span>\n                  </el-checkbox>\n                  <el-checkbox label=\"high\">\n                    <span class=\"severity-checkbox\">\n                      <span class=\"severity-dot high\"></span>\n                      <span>高危</span>\n                    </span>\n                  </el-checkbox>\n                  <el-checkbox label=\"medium\">\n                    <span class=\"severity-checkbox\">\n                      <span class=\"severity-dot medium\"></span>\n                      <span>中危</span>\n                    </span>\n                  </el-checkbox>\n                  <el-checkbox label=\"low\">\n                    <span class=\"severity-checkbox\">\n                      <span class=\"severity-dot low\"></span>\n                      <span>低危</span>\n                    </span>\n                  </el-checkbox>\n                  <el-checkbox label=\"info\">\n                    <span class=\"severity-checkbox\">\n                      <span class=\"severity-dot info\"></span>\n                      <span>信息</span>\n                    </span>\n                  </el-checkbox>\n                </el-checkbox-group>\n              </div>\n            </el-form-item>\n\n            <el-form-item>\n              <el-button type=\"primary\" @click=\"generateReport\" :loading=\"generating\" class=\"generate-btn\">\n                <i class=\"el-icon-document-add\"></i> 生成报告\n              </el-button>\n            </el-form-item>\n          </el-form>\n        </div>\n      </el-card>\n    </div>\n\n    <!-- 最近报告列表卡片 -->\n    <div class=\"recent-reports-container\">\n      <el-card shadow=\"hover\" class=\"recent-reports-card\">\n        <template #header>\n          <div class=\"card-header\">\n            <h2 class=\"section-title\">\n              <i class=\"el-icon-tickets\"></i>\n              最近生成的报告\n            </h2>\n          </div>\n        </template>\n\n        <el-table \n          :data=\"reports\" \n          style=\"width: 100%\"\n          v-loading=\"loading\"\n          class=\"reports-table\"\n          :header-cell-style=\"{ background: '#f5f7fa', color: '#606266' }\"\n        >\n          <el-table-column prop=\"name\" label=\"报告名称\" min-width=\"220\">\n            <template #default=\"scope\">\n              <div class=\"report-name\">\n                <i class=\"el-icon-document report-icon\" :class=\"scope.row.type\"></i>\n                <span>{{ scope.row.name }}</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"type\" label=\"类型\" width=\"120\">\n            <template #default=\"scope\">\n              <el-tag :type=\"getReportTagType(scope.row.type)\" effect=\"light\">\n                {{ getReportTypeText(scope.row.type) }}\n              </el-tag>\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"format\" label=\"格式\" width=\"100\">\n            <template #default=\"scope\">\n              <span class=\"format-badge\" :class=\"scope.row.format\">\n                {{ scope.row.format.toUpperCase() }}\n              </span>\n            </template>\n          </el-table-column>\n          <el-table-column prop=\"createdAt\" label=\"生成时间\" width=\"180\">\n            <template #default=\"scope\">\n              <div class=\"date-info\">\n                <i class=\"el-icon-time date-icon\"></i>\n                <span>{{ formatDate(scope.row.createdAt) }}</span>\n              </div>\n            </template>\n          </el-table-column>\n          <el-table-column label=\"操作\" width=\"180\" fixed=\"right\">\n            <template #default=\"scope\">\n              <div class=\"table-actions\">\n                <el-button \n                  size=\"small\" \n                  type=\"primary\" \n                  @click=\"downloadReport(scope.row)\"\n                  class=\"action-btn download-btn\"\n                >\n                  <i class=\"el-icon-download\"></i> 下载\n                </el-button>\n                <el-button \n                  size=\"small\" \n                  type=\"danger\" \n                  @click=\"confirmDelete(scope.row)\"\n                  class=\"action-btn delete-btn\"\n                >\n                  <i class=\"el-icon-delete\"></i> 删除\n                </el-button>\n              </div>\n            </template>\n          </el-table-column>\n        </el-table>\n\n        <div v-if=\"reports.length === 0 && !loading\" class=\"no-reports\">\n          <el-empty description=\"暂无报告记录\">\n            <template #description>\n              <p>暂无生成的报告记录，请使用上方工具生成新报告</p>\n            </template>\n            <el-button type=\"primary\" @click=\"scrollToGenerator\">生成报告</el-button>\n          </el-empty>\n        </div>\n      </el-card>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { ref, reactive } from 'vue'\nimport { ElMessage, ElMessageBox } from 'element-plus'\n\nexport default {\n  name: 'Reports',\n  \n  setup() {\n    // 加载状态\n    const loading = ref(false)\n    const generating = ref(false)\n    \n    // 报告生成表单\n    const reportForm = reactive({\n      type: 'summary',\n      format: 'pdf',\n      dateRange: [],\n      severities: ['critical', 'high', 'medium']\n    })\n    \n    // 模拟报告数据\n    const reports = ref([\n      {\n        id: '1',\n        name: '2023年第二季度漏洞摘要报告',\n        type: 'summary',\n        format: 'pdf',\n        createdAt: '2023-07-01T08:30:00Z',\n        url: '#'\n      },\n      {\n        id: '2',\n        name: '网络安全合规性检查报告',\n        type: 'compliance',\n        format: 'word',\n        createdAt: '2023-06-15T14:20:00Z',\n        url: '#'\n      },\n      {\n        id: '3',\n        name: '漏洞详细分析报告',\n        type: 'detailed',\n        format: 'excel',\n        createdAt: '2023-06-10T11:45:00Z',\n        url: '#'\n      }\n    ])\n    \n    // 获取报告类型文本\n    const getReportTypeText = (type) => {\n      const typeMap = {\n        summary: '摘要报告',\n        detailed: '详细报告',\n        compliance: '合规报告',\n        trend: '趋势分析'\n      }\n      return typeMap[type] || type\n    }\n    \n    // 获取报告标签类型\n    const getReportTagType = (type) => {\n      const typeMap = {\n        summary: '',\n        detailed: 'success',\n        compliance: 'warning',\n        trend: 'info'\n      }\n      return typeMap[type] || ''\n    }\n    \n    // 格式化日期\n    const formatDate = (dateString) => {\n      const date = new Date(dateString)\n      return date.toLocaleDateString('zh-CN', {\n        year: 'numeric',\n        month: '2-digit',\n        day: '2-digit',\n        hour: '2-digit',\n        minute: '2-digit'\n      })\n    }\n    \n    // 滚动到报告生成器\n    const scrollToGenerator = () => {\n      document.querySelector('.report-card').scrollIntoView({ \n        behavior: 'smooth',\n        block: 'start'\n      })\n    }\n    \n    // 生成报告\n    const generateReport = () => {\n      if (!reportForm.dateRange || reportForm.dateRange.length !== 2) {\n        ElMessage.warning('请选择报告时间范围')\n        return\n      }\n      \n      if (reportForm.severities.length === 0) {\n        ElMessage.warning('请至少选择一个严重程度级别')\n        return\n      }\n      \n      generating.value = true\n      \n      // 模拟报告生成过程\n      setTimeout(() => {\n        generating.value = false\n        \n        const reportTypes = {\n          summary: '漏洞摘要报告',\n          detailed: '详细漏洞报告',\n          compliance: '合规性报告',\n          trend: '趋势分析报告'\n        }\n        \n        // 创建新报告对象\n        const newReport = {\n          id: Date.now().toString(),\n          name: `${new Date().getFullYear()}年${reportTypes[reportForm.type]}`,\n          type: reportForm.type,\n          format: reportForm.format,\n          createdAt: new Date().toISOString(),\n          url: '#'\n        }\n        \n        // 添加到报告列表\n        reports.value.unshift(newReport)\n        \n        ElMessage.success('报告生成成功')\n      }, 2000)\n    }\n    \n    // 下载报告\n    const downloadReport = (report) => {\n      // 实际应用中，这里应该进行实际的下载操作\n      ElMessage.success(`开始下载：${report.name}.${report.format}`)\n    }\n    \n    // 确认删除\n    const confirmDelete = (report) => {\n      ElMessageBox.confirm(\n        `确定要删除报告\"${report.name}\"吗？`,\n        '警告',\n        {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        }\n      ).then(() => {\n        deleteReport(report.id)\n      }).catch(() => {\n        // 用户取消删除\n      })\n    }\n    \n    // 删除报告\n    const deleteReport = (id) => {\n      const index = reports.value.findIndex(r => r.id === id)\n      if (index !== -1) {\n        reports.value.splice(index, 1)\n        ElMessage.success('报告已删除')\n      }\n    }\n    \n    return {\n      loading,\n      generating,\n      reportForm,\n      reports,\n      getReportTypeText,\n      getReportTagType,\n      formatDate,\n      generateReport,\n      downloadReport,\n      confirmDelete,\n      scrollToGenerator\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.reports-page {\n  padding: 24px;\n  animation: fadeIn 0.6s ease;\n}\n\n@keyframes fadeIn {\n  from { opacity: 0; transform: translateY(10px); }\n  to { opacity: 1; transform: translateY(0); }\n}\n\n.page-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  margin-bottom: 24px;\n  \n  .header-content {\n    flex: 1;\n  }\n  \n  .page-title {\n    font-size: 28px;\n    font-weight: 600;\n    margin: 0 0 8px 0;\n    display: flex;\n    align-items: center;\n    color: #303133;\n    \n    .title-icon {\n      margin-right: 12px;\n      font-size: 28px;\n      color: #409EFF;\n    }\n    \n    .title-highlight {\n      background: linear-gradient(120deg, #409EFF, #53A8FF);\n      -webkit-background-clip: text;\n      -webkit-text-fill-color: transparent;\n      margin-left: 8px;\n      font-weight: 700;\n    }\n  }\n  \n  .page-subtitle {\n    color: #606266;\n    font-size: 16px;\n    margin: 0;\n  }\n}\n\n.report-card-container,\n.recent-reports-container {\n  margin-bottom: 24px;\n}\n\n.report-card,\n.recent-reports-card {\n  border-radius: 12px;\n  box-shadow: 0 4px 16px rgba(0, 0, 0, 0.05);\n  transition: all 0.3s;\n  position: relative;\n  overflow: hidden;\n  \n  &:hover {\n    box-shadow: 0 8px 24px rgba(0, 0, 0, 0.09);\n  }\n  \n  &::before {\n    content: '';\n    position: absolute;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 4px;\n    background: linear-gradient(90deg, #409EFF, #53A8FF);\n  }\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 4px 0;\n}\n\n.section-title {\n  font-size: 18px;\n  font-weight: 600;\n  margin: 0;\n  color: #303133;\n  display: flex;\n  align-items: center;\n  \n  i {\n    margin-right: 8px;\n    font-size: 18px;\n    color: #409EFF;\n  }\n}\n\n.report-options {\n  padding: 16px 0;\n}\n\n.report-option {\n  display: flex;\n  align-items: center;\n  \n  .option-icon {\n    margin-right: 8px;\n    font-size: 16px;\n    width: 20px;\n    height: 20px;\n    text-align: center;\n    line-height: 20px;\n    \n    &.summary {\n      color: #409EFF;\n    }\n    \n    &.detailed {\n      color: #67C23A;\n    }\n    \n    &.compliance {\n      color: #E6A23C;\n    }\n    \n    &.trend {\n      color: #9c27b0;\n    }\n    \n    &.pdf {\n      color: #F56C6C;\n    }\n    \n    &.excel {\n      color: #67C23A;\n    }\n    \n    &.word {\n      color: #409EFF;\n    }\n    \n    &.html {\n      color: #E6A23C;\n    }\n  }\n}\n\n.date-range-picker {\n  :deep(.el-input__inner) {\n    border-radius: 8px;\n  }\n}\n\n.severity-checkboxes {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 16px;\n  \n  .severity-checkbox {\n    display: flex;\n    align-items: center;\n    \n    .severity-dot {\n      width: 10px;\n      height: 10px;\n      border-radius: 50%;\n      margin-right: 8px;\n      \n      &.critical {\n        background-color: #F44336;\n      }\n      \n      &.high {\n        background-color: #FF9800;\n      }\n      \n      &.medium {\n        background-color: #FFEB3B;\n      }\n      \n      &.low {\n        background-color: #4CAF50;\n      }\n      \n      &.info {\n        background-color: #2196F3;\n      }\n    }\n  }\n}\n\n.gener